  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch CV Development",
      "request": "launch",
      "type": "dart",
      "cwd": "apps/cv",
      "program": "lib/main_development.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main_development.dart"
      ]
    },

    {
      "name": "Launch CV Staging",
      "request": "launch",
      "type": "dart",
      "cwd": "apps/cv",
      "program": "lib/main_staging.dart",
      "args": ["--flavor", "staging", "--target", "lib/main_staging.dart"]
    },

    {
      "name": "Launch CV Production",
      "request": "launch",
      "type": "dart",
      "cwd": "apps/cv",
      "program": "lib/main_production.dart",
      // "flutterMode": "release",
      "flutterMode": "debug",
      "args": ["--flavor", "production", "--target", "lib/main_production.dart"]
    }
  ]
}
