  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Development",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "args": ["--flavor", "development"]
    },

    {
      "name": "Launch Staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_staging.dart",
      "args": ["--flavor", "staging"]
    },

    {
      "name": "Launch Production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_production.dart",
      "flutterMode": "release",
      "args": ["--flavor", "production"]
    }
  ]
}
